<?xml version="1.0" encoding="utf-8"?>
<root>
    <!-- Name of the agent. -->
    <Name>MyAgent</Name>

    <!-- Command to run, BlueAir will put the found file into $path$, 
    the file's URL to $url$ and file name (if given, otherwise the target directory) to $file$. -->
    <Command>$path$ --url $url$ --save $file$</Command>

    <!-- Files to look for this agent. -->
    <Files>
        <File>/usr/bin/myagent</File>
        <File>C:/Program Files/MyAgent/MyAgent.exe</File>
    </Files>
</root>